{
    "name": "covid",
    "initialization": {
        "type": "absolute",
        "value": 10,
        "distribution": "random",
        "state": "asymptomatic"
    },
    "attributes": {
        "symptoms": ["cough", "loss of taste"]
    },
    "transition_states": {
        "exposed": {
            "asymptomatic": {
                "scale": "per_day",
                "probability": 0.1
            }
        }, 
        "asymptomatic": {
            "symptomatic": {
                "scale": "per_day",
                "probability": 0.1
            },
            "recovered": {
                "scale": "per_day",
                "probability": 0.1
            }
        }, 
        "symptomatic": {
            "severe": {
                "scale": "per_day",
                "probability": 0.1
            },
            "recovered": {
                "scale": "per_day",
                "probability": 0.1
            }
        },
        "severe": {
            "recovered": {
                "scale": "per_day",
                "probability": 0.1
            },
            "dead": {
                "scale": "per_day",
                "probability": 0.1
            }
        },
        "recovered": {
            "susceptible": {
                "scale": "per_day",
                "probability": 0.1
            }
        }
    }
}